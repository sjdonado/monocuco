{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module",
    "project": ["tsconfig.json"]
  },
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "ignorePatterns": ["**/*.js"],
  "rules": {}
}